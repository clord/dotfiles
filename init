#!/bin/zsh
wdir=`pwd`
git submodule init
git submodule update
ln -fs $pwd/zshrc ~/.zshrc
ln -fs $pwd/vimrc ~/.vimrc
ln -fs $pwd/irbrc ~/.irbrc
ln -fs $pwd/vim ~/.vim
ln -fs $pwd/zshrc ~/.zshrc
ln -fs $pwd/zshenv ~/.zshenv
ln -fs $pwd/zprofile ~/.zprofile
ln -fs $pwd/zsh ~/.zsh
mkdir -p ~/tmp
touch ~/tmp/user.vim

